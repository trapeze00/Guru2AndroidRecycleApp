<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.guru2androidrecycleapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Guru2AndroidRecycleApp">
        <activity android:name=".PageTipPlastic"></activity>
        <activity android:name=".PageTipPaper" />
        <activity android:name=".pageQuizStyrofoam" />
        <activity android:name=".pageQuizVinyl" />
        <activity android:name=".pageQuizCan" />
        <activity android:name=".pageQuizGlass" />
        <activity android:name=".pageQuizPlastic" />
        <activity android:name=".pageQuizPaper" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".BookmarkActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".MyPage" />
        <activity android:name=".PageSignUp" />
        <activity android:name=".BeforeLoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>